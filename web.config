<configuration>
  <system.webServer>
    <rewrite>
        <rules>
            <rule name="Redirect to non-www" stopProcessing="true">
                <match url="(.*)" negate="false"></match>
                <action type="Redirect" url="http://stuwork.com/{R:1}"></action>
                <conditions>
                    <add input="{HTTP_HOST}" pattern="^stuwork\.com$" negate="true"></add>
                </conditions>
            </rule>
        </rules>
    </rewrite>
  </system.webServer> 
</configuration>