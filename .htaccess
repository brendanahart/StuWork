//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^stuwork.com[nc]
RewriteRule ^(.*)$ http://www.stuwork.com/$1 [r=301,nc]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]

//Custom 400 errors
ErrorDocument 400 /site/wwwroot/404.html

//Custom 401 errors
ErrorDocument 401 /site/wwwroot/404.html

//Custom 403 errors
ErrorDocument 403 /site/wwwroot/404.html

//Custom 404 errors
ErrorDocument 404 /site/wwwroot/404.html

//Custom 500 errors
ErrorDocument 500 /site/wwwroot/404.html

//Change default directory page
DirectoryIndex index.html

//Prevent viewing of .htaccess file
<Files .htaccess>
order allow,deny
deny from all
</Files>
 